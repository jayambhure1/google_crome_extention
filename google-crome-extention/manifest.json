{
  "manifest_version": 2,
  "name": "My Social Profile",
  "description": "Easy access Facebook,Instagram and Other Social URL",
  "version": "1.0",
  "icons": {
    "128": "images/logo.png"
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}